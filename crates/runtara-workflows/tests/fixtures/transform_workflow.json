      "stepType": "Agent",
      "id": "transform",
      "agentId": "transform",
      "capabilityId": "map-fields",
      "inputMapping": {
        "source": {
          "valueType": "reference",
          "value": "data"
        },
        "mapping": {
          "valueType": "immediate",
          "value": {
            "output_field": "$.input_field"
          }
        }
      }
    },
    "finish": {
      "stepType": "Finish",
      "id": "finish",
      "inputMapping": {
        "result": {
          "valueType": "reference",
          "value": "steps.transform.outputs"
        }
      }
    }
  },
  "entryPoint": "transform",
  "executionPlan": [
    {
      "fromStep": "transform",
      "toStep": "finish"
    }
  ],
